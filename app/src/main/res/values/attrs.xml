<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSliderView">
        <attr name="titleText" format="string"/>
        <attr name="value" format="float"/>
        <attr name="enabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomItemView">
        <attr name="listTitle" format="string"/>
        <attr name="listSubtitle" format="string"/>
        <attr name="listIcon" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CustomSwitchView">
        <attr name="switchTitle" format="string"/>
        <attr name="switchSubtitleOn" format="string"/>
        <attr name="switchSubtitleOff" format="string"/>
        <attr name="defaultValue" format="boolean"/>
        <attr name="key" format="string" />
    </declare-styleable>
</resources>